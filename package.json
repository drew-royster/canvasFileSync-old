{
  "name": "canvas-file-sync",
  "version": "1.0.0",
  "description": "Syncs files from canvas to your local machine",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager .",
    "build-mac": "electron-packager . --overwrite --out=build --icon=icons_normal/icons/mac/icon.icns",
    "build-windows": "electron-packager . --overwrite --out=build --icon=icons_normal/icons/win/icon.ico",
    "build-linux": "electron-packager . --overwrite --out=build --icon=icons_normal/icons/png/48x48.png",
    "test": "echo 'no test specified' && exit 1"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Drew Royster",
  "productName": "Canvas File Sync",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^2.0.3",
    "electron-debug": "^2.0.0",
    "electron-icon-maker": "0.0.4",
    "electron-packager": "^12.1.0",
    "icon-convert": "^1.0.0"
  },
  "dependencies": {
    "bulma": "^0.7.1",
    "moment": "^2.22.2",
    "request": "^2.87.0",
    "request-promise": "^4.2.2"
  }
}
