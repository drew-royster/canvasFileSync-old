{
  "name": "canvas-file-sync",
  "version": "1.0.0",
  "description": "Syncs files from canvas to your local machine",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager .",
    "build-mac": "electron-packager . --platform=darwin --asar --overwrite --app-bundle-id=\"com.drewroyster.canvasfilesync\" --app-verions=\"1.0.0\" --build-version=\"1.0.100\" --osx-sign --out=build --icon=icons_normal/icons/mac/icon.icns",
    "build-windows": "electron-packager . --platform=win32 --asar --overwrite --out=build --icon=icons_normal/icons/win/icon.ico",
    "build-linux": "electron-packager . --platform=linux --asar --overwrite --out=build --icon=icons_normal/icons/png/48x48.png",
    "test": "mocha"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Drew Royster",
  "productName": "Canvas File Sync",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^2.0.7",
    "electron-debug": "^2.0.0",
    "electron-icon-maker": "0.0.4",
    "electron-packager": "^12.1.0",
    "electron-winstaller": "^2.6.4",
    "icon-convert": "^1.0.0",
    "mocha": "^5.2.0",
    "spectron": "^3.8.0"
  },
  "dependencies": {
    "electron-log": "^2.2.16",
    "electron-squirrel-startup": "^1.0.0",
    "moment": "^2.22.2",
    "request": "^2.87.0",
    "request-promise": "^4.2.2"
  }
}
